<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TestNG AllTestSuite" configfailurepolicy="continue" verbose="1" time-out="300000">
   <test name="restapi - test priorities" >
      <packages>
         <package name="org.alfresco.rest.rm.community.records.priorities" />
      </packages>
   </test>
   <test name="restapi">
      <packages>
         <package name="org.alfresco.rest.rm.community.*">
            <exclude name="org.alfresco.rest.rm.community.records.priorities" />
         </package>
      </packages>
   </test>
</suite>
