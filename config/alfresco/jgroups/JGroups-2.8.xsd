<xs:annotation>
<xs:documentation>The max number of bytes in a message. Larger messages will be fragmented. Default is 8192 bytes</xs:documentation>
</xs:annotation>
</xs:attribute>
<xs:attribute name="max_retained_buffer" type="xs:string">
