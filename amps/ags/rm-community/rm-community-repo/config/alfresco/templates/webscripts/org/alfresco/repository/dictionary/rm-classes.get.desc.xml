<webscript>
   <shortname>Get RM/DM related class definitions</shortname>
   <description>Gets the RM/DM related collection of class definitions - parameters classfilter, namespaceprefix and name.</description>
   <url>/api/rm/classes?cf={classFilter?}&amp;nsp={namespacePrefix?}&amp;n={name?}</url>
   <format default="json">argument</format>
   <authentication>user</authentication>
   <transaction allow="readonly">required</transaction>
</webscript>