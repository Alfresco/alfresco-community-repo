    <profiles>
        <profile>
            <id>community</id>
            <modules>
                <module>war</module>
            </modules>
        </profile>
        <profile>
            <id>communityDocker</id>
            <modules>
                <module>war</module>
                <module>docker-alfresco</module>
            </modules>
        </profile>
        <profile>
            <id>fullBuild</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>war</module>
                <module>distribution</module>
                <module>public-javadoc</module>
                <module>docker-alfresco</module>
            </modules>
        </profile>
        <profile>
            <id>all-tas-tests</id>
            <modules>
